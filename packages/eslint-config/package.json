{
  "name": "@theguild/eslint-config",
  "version": "0.2.0",
  "type": "commonjs",
  "description": "The Guild's shared ESLint config",
  "repository": {
    "url": "the-guild-org/shared-config",
    "directory": "packages/eslint-config"
  },
