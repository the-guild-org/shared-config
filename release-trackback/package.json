{
  "name": "release-trackback-action",
  "version": "0.0.0",
  "description": "Utility to track back a release to a PR, and notify in case of a breaking change",
  "repository": {
    "type": "git",
    "directory": "release-trackback",
    "url": "git+https://github.com/the-guild-org/shared-config.git"
  },
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "private": true,
  "main": "lib/main.js",
  "keywords": [
    "theguild",
    "release",
    "actions"
  ],
  "scripts": {
    "all": "npm run build && npm run package",
    "build": "tsc",
    "package": "ncc build"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "marked": "5.1.1",
    "semver": "7.5.4"
  },
  "devDependencies": {
    "@types/marked": "5.0.0",
    "@types/node": "18.16.19",
    "@types/semver": "7.5.0",
    "@vercel/ncc": "0.36.1",
    "typescript": "5.1.6"
  }
}
